{
	"$schema": "http://json.schemastore.org/template",
	"author": "Oisin Haken, David Guerin",
	"classifications": [ "Web", "WebAPI" ],
	"identity": "Eshopworld.WebAPIFabric.Template",
	"name": "EshopWorld Web API Service Fabric hosted ASP.NET Core 2 template",
	"shortName": "ESWWebAPI",
	"symbols": 
	 {
		"APIName": 
		{
			"type": "parameter",
			"datatype": "string",			
			"replaces": "WebAPIService",
			"fileRename": "WebAPIService",
			"isRequired": true
		},
		"APIPortNumber": 
		{
			"type": "parameter",
			"datatype": "integer",
			"isRequired": true,
			"replaces": "WebAPIServicePort"
		},
		"APIHttpsPortNumber": 
		{
			"type": "parameter",
			"datatype": "integer",
			"isRequired": true,
			"replaces": "WebHttpsAPIServicePort"
		}
	},
	"sources": 
	[{
		"exclude": [
		"**/[Bb]in/**",
		"**/[Oo]bj/**",
		".template.config/**/*",
		"**/*.filelist",
		"**/*.user",
		"**/*.lock.json",
		"src/.vs/**"
	]}]
}